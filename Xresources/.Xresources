! xft {{{
!!Xft*dpi: 92
!!Xft*antialias: true
!!!Xft*hinting: true
!!Xft*autohint: true
!!!Xft*hintstyle: hintfull
!!Xft*rgba: vbgr
!Xft*rgba: rgba
!}}}

#define FONT_NORMAL         xft:Inconsolata\\-g for Powerline:pixelsize=12:embolden=1,xft:Source Code Pro:pixelsize=13:embolden=1:style=SemiBold
#define FONT_BOLD           xft:Inconsolata\\-g for Powerline:pixelsize=12:embolden=1:style=Bold,xft:Source Code Pro:pixelsize=13:style=Bold:embolden=1
#define FONT_ITALIC         xft:Source Code Pro:pixelsize=13:embolden=1:style=Black
#define FONT_BOLDITALIC     xft:Source Code Pro:pixelsize=13:embolden=1:style=Semibold Italic

vte*font:       FONT_NORMAL
vte*boldFont:   FONT_BOLD
vte*italicFont: FONT_ITALIC
vte*background: #121212
vte*foreground: #d6d6d6
vte*scrollBar:  false

!vim*font:                         FONT_NORMAL
!vim*boldFont:                     FONT_BOLD
!vim*italicFont:                   FONT_ITALIC
!vim*boldItalicFont:               FONT_BOLDITALIC
!vim*background:                   #080808
!vim*foreground:                   #ebebeb
vim*scrollBar:                    false
vim.iso14755:             1
vim.iso14755_52:          1
vim.skipBuiltinGlyphs:    1
vim.insecure:             1

mongolia.iso14755:             1
mongolia.iso14755_52:          1
mongolia.skipBuiltinGlyphs:    0
mongolia.insecure:             1

mongolia.buffered:             0
mongolia.internalBorder:       0
mongolia.externalBorder:       0

mongolia.font:             FONT_NORMAL
mongolia.boldFont:         FONT_BOLD
mongolia.italicFont:       xft:Profont:embolden=1
mongolia.boldItalicFont:   FONT_BOLDITALIC

yemen.iso14755:             1
yemen.iso14755_52:          1
yemen.skipBuiltinGlyphs:    0
yemen.insecure:             1

yemen.buffered:             0
yemen.internalBorder:       0
yemen.externalBorder:       0

yemen.font:                 xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1
yemen.boldFont:             xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1:style=Bold
yemen.italicFont:           xft:Profont for Powerline:pixelsize=16:embolden=1
yemen.boldItalicFont:       -misc-fixed-medium-normal--7-70-75-75-c-50-koi8

yemen*cutchars:             "()*,<>[]{}|'"
yemen*matcher.pattern.1:    \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
yemen*matcher.button:       1
yemen*matcher.launcher.1:   /home/rachel/bin/ff
yemen*underlineURLs:        false
yemen*urlLauncher:          /home/rachel/bin/ff

yemen.jumpScroll:           true
yemen.loginShell:           true
yemen.saveLines:            4096
yemen.scrollBack:           true
yemen.scrollKey:            true
yemen.scrollTtyKeypress:    true
yemen.scrollTtyOutput:      false
yemen.scrollWithBuffer:     true
yemen.skipScroll:           true
yemen.trippleclickwords:    true
yemen.urgentOnBell:         false

yemen.pasteableTabs:        false
yemen.pointerBlank:         false
yemen.pointerBlankDelay:    2
yemen.scrollstyle:          next
yemen.scrollBar:            false
yemen.depth:                24

mongolia*cutchars:             "()*,<>[]{}|'"
mongolia*matcher.pattern.1:    \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
mongolia*matcher.button:       1
mongolia*matcher.launcher.1:   /home/rachel/bin/ff
mongolia*underlineURLs:        false
mongolia*urlLauncher:          /home/rachel/bin/ff

mongolia.jumpScroll:           true
mongolia.loginShell:           true
mongolia.saveLines:            4096
mongolia.scrollBack:           true
mongolia.scrollKey:            true
mongolia.scrollTtyKeypress:    true
mongolia.scrollTtyOutput:      false
mongolia.scrollWithBuffer:     true
mongolia.skipScroll:           true
mongolia.trippleclickwords:    true
mongolia.urgentOnBell:         false

mongolia.pasteableTabs:        false
mongolia.pointerBlank:         false
mongolia.pointerBlankDelay:    2
mongolia.scrollstyle:          next
mongolia.scrollBar:            false
mongolia.depth:                24

URxvt.perl-ext-common:      default,clipboard,selection-to-clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  ff
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.Shift-Control-C : perl:clipboard:copy
URxvt.keysym.Shift-Control-V : perl:clipboard:paste

