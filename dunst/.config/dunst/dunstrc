[global]
    # https://github.com/catppuccin/dunst
    frame_color = "#89B4FA"
    separator_color= frame

    font = Inconsolata 14
    word_wrap = yes
    padding = 8
    horizontal_padding = 8
    separator_color = auto
    timeout = 10
    follow = keyboard
    icon_position = off

    width = (0, 700)
    height = 800
    origin = top-right
    offset = 15x15
    scale = 0
    notification_limit = 5
    frame_width = 0
    show_indicators = no
    stack_duplicates = no

    close_all = mod4+space
    history = mod4+h

    [urgency_low]
    # https://github.com/catppuccin/dunst
    background = "#1E1E2E"
    foreground = "#CDD6F4"
    timeout = 5

    [urgency_normal]
    # https://github.com/catppuccin/dunst
    background = "#1E1E2E"
    foreground = "#CDD6F4"

    [urgency_critical]
    # https://github.com/catppuccin/dunst
    background = "#1E1E2E"
    foreground = "#CDD6F4"
    frame_color = "#FAB387"
    timeout = 0

