#!/bin/zsh
if [ "$HOST" = "mongolia" ]; then
    xrandr --output DVI-I-1 --mode 1920x1080 \
           --output DP-1.1.8 --primary --right-of DVI-I-1 --mode 1920x1080 \
           --output DP-1.8 --right-of DP-1.1.8 --mode 1920x1080 \
           --output HDMI-0 --off
elif [ "$HOST" = "yemen" ]; then
    xrandr --output eDP-1 --mode 1920x1080 
fi
